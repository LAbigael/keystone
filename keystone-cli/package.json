{
  "name": "@keystone/cli",
  "description": "Decentralized workspaces for developers to store, share and use secrets securely.",
  "version": "0.0.1",
  "author": "Samuel Roy",
  "bin": {
    "secret": "./bin/run"
  },
  "bugs": "https://github.com/keystone.sh/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://keystone.sh",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "ks",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/keystone.sh",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "jest --runInBand",
    "lint": "eslint",
    "pack": "oclif-dev pack",
    "version": "oclif-dev readme && git add README.md",
    "build": ""
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@keystone/core": "~0.0.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.19.0",
    "blockstack": "21.0.0-alpha.4",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.0",
    "daffy": "^1.0.3",
    "elliptic": "^6.4.1",
    "filename-regex": "^2.0.1",
    "filter-console": "^0.1.1",
    "inquirer": "^6.4.1",
    "is-email": "^1.0.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.15",
    "node-cache": "^5.0.2",
    "normalize-path": "^3.0.0",
    "object-hash": "^1.3.1",
    "open": "^6.4.0",
    "three-way-merge": "^0.1.0",
    "treeify": "^1.1.0",
    "uuid": "^3.3.3",
    "walkdir": "^0.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "debug": "^4.1.1",
    "eslint": "^5.13",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-oclif": "^3.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "globby": "^8",
    "jest": "^24.9.0",
    "mock-stdin": "^0.3.1",
    "prettier": "1.18.2",
    "user-home": "^2.0.0"
  }
}
